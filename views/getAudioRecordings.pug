doctype html
html
  head
    title Get Audio Recordings
    include includes/base.pug
    include includes/bootstrap3.pug
    script(type='text/javascript', src='javascripts/getAudioRecordings.js')

  body
    include includes/navbar-bootstrap3.pug
    .container-flex
      include includes/getQuery.pug

    .container-flex
      table#results-table(class="table table-striped")
        tr
          th View
          th ID
          th Group
          th Device ID
          th Location
          th Time
          th Date
          th Duration
          th BatteryLevel
          th BatteryCharging
          th AirplaneMode
          th File
          th Delete
